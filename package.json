{
    "name": "peculiarLibrary",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@dynatrace/oneagent": "^1.219.134-1.0.0",
        "@dynatrace/oneagent-sdk": "^1.4.1",
        "@sap/cds": "^5",
        "@sap/hana-client": "^2.6.61",
        "axios": "^0.21.1",
        "express": "^4"
    },
    "devDependencies": {
        "sqlite3": "^5.0.2"
    },
    "scripts": {
        "start": "cds run"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "env": {
            "es2020": true,
            "node": true,
            "jest": true,
            "mocha": true
        },
        "globals": {
            "SELECT": true,
            "INSERT": true,
            "UPDATE": true,
            "DELETE": true,
            "CREATE": true,
            "DROP": true,
            "CDL": true,
            "CQL": true,
            "CXL": true,
            "cds": true
        },
        "rules": {
            "no-console": "off",
            "require-atomic-updates": "off"
        }
    },
    "cds": {
        "DT_INJECTION_RULES": "::INCLUDE:ENDS,NODEJS_APP_BASE_DIR,/node_modules/npm",
        "requires": {
            "db": {
                "kind": "sql"
            }
        },
        "hana": {
            "deploy-format": "hdbtable"
        }
    }
}
